; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="NNViz"
run/main_scene="res://project/scenes/main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://project/icon.svg"

[autoload]

AntialiasedLine2DTexture="*res://addons/antialiased_line2d/texture.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1440
window/size/window_height_override=810

[editor]

movie_writer/movie_file="D:/Godot Games/NeuralNetVisualizer/movies/example.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/antialiased_line2d/plugin.cfg", "res://addons/easy_charts/plugin.cfg", "res://addons/gut/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
viewport/hdr_2d=true
